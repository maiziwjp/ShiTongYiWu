		</header>
		
</body>
<script type="text/javascript" src="../../script/api.js"></script>
<script type="text/javascript">
	apiready = function(){
	frame_flag=true;
		$api.fixStatusBar($api.dom('header'));
		var height=document.getElementById('winpart').offsetHeight;
		api.openFrame({
