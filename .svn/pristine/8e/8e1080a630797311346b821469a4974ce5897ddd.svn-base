	function doMove ( obj, attr, dir, target ) {
		clearInterval( obj.timer );
		obj.timer = setInterval(function () {
			var t1=parseInt(getStyle( obj, attr ));
			var speed = parseInt(getStyle( obj, attr )) + dir;			// 步长
			if(t1!=239)
			{
				obj.style.opacity="0";
			}
			else
			{
				obj.style.opacity="1";
			}
			if(speed==target)
			{
				obj.style[attr] = 239 + 'px';
