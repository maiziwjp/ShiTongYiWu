<body>
	<div id="header">
        	<div id="search">
            	<div id="back"><img src="../../icon/public/back_black.png"/ ></div>
            	<div id="se">
            	     我的评价
            	</div>
            	<div class="back"></div>
          </div>
   </div>

	 <div id="four_order">
      <div class="none" style="height:100%;width:5%;"></div>
			<div id="username" style="height:80%;position:relative;top:10%;width:45%;display: -webkit-box;">
				   <img class="sh_store_image"src="../../icon/public/store.png"	style="width: 30px;height:auto"}/ >
					 <div style="font-size:15px;margin-left:8%;margin-top:4%;">世通用户</div>
				</div>

			<div id="gocomment" style="height:80%;position:relative;top:10%;width:43%;display: -webkit-box;">
            <div class="none1" style="height:100%;width:50%;"></div>
						<div style="height:100%;width:50%;"><button class="order_comment"id="myorder_5">去评价</button></div>
			</div>

	 </div>

</body>
<script type="text/javascript" src="../../script/api.js"></script>
<script type="text/javascript">
	apiready = function(){
		var headerHeight;
	    var systemType = api.systemType;
	    var height=document.getElementById("search").offsetHeight;
			var height1=document.getElementById('header').offsetHeight;
		if(systemType == "ios")
		{
		    header.style.top="45px";
		    headerHeight=40;
		}
		else
		{
			header.style.top="45px";
		    headerHeight=45;
		}
		var fiveorderPos = $api.offset(four_order);
		api.openFrame({
			name: 'money_exchange',
			url: './my_comment_all_frame.html',
				bounces: true,
				rect: {
						x: 0,
						y: height1+fiveorderPos.h +20,

				},
				vScrollBarEnabled:false,
				hScrollBarEnabled:false,
		});

        /*关闭顶部搜索栏*/
        document.getElementById("back").onclick=function(){
        	api.closeWin();
        };
        document.getElementById('myorder_5').onclick = function() {
			api.openWin({
				name : 'myorder_win',
				url : './myorder_win.html',
				bounces : false,
				rect : {
					x : 0,
					w : 'auto',
				}
			});
			$api.setStorage('myorder_flag', 5);
		};
	};

</script>
